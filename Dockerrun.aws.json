
{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "mado7abib007/swfront",
            "hostname": "nginx",
            "essential": true,
            "memory":10
        },
        {
            "name": "backend",
            "image": "mado7abib007/swbackend",
            "hostname": "backend",
            "essential": true,
            "memory":500
        },
        {
            "name": "router",
            "image": "mado7abib007/router",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],

            "links":["backend", "frontend"],
            "memory":10
        }
       
    ]
}